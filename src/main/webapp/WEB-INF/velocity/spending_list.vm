#tiles_insertTemplate({"template":"template.vm"})
	#tiles_putAttribute({"name":"title", "value": "Lista wydatków"})#end
	#tiles_putAttribute({"name": "navigation"})
		<ul class="nav nav-list">
			<li><a href="#springUrl("/spending_insert.html")">Dodawanie wydatków</a></li>
			<li class="active"><a href="#springUrl("/spending_list.html")">Lista</a></li>
